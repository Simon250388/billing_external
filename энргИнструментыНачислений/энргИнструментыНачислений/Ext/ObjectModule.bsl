				СтрокаНабора.ЗначениеТарифа 				= ?(НЕ ИспользоватьТарифРек, ЗначениеТарифа.Значение, ЗначениеТарифа.ЗначениеТарифаРЭК);
				//СтрокаНабора.ЗначениеТарифа 				= 0; было
				СтрокаНабора.Поставщик 						= ДанныеПомещения.Поставщик;
