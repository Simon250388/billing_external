Процедура СтабПериодыНПУстановитьФормулуРасчетаТочекУчета(ссылка,Параметры) 
	МассивТочек 								= ссылка.ТочкиУчета.выгрузитьКолонку("ТочкаУчета");	
	МассивТочек.добавить(ссылка.РассчитываемаяТочкаУчета);
	ПараметрыЗапроса  							= Параметры.ПараметрыЗапроса;	
	МВТ 										= Новый МенеджерВременныхТаблиц;
	Запрос  									= Новый Запрос;
	Запрос.УстановитьПараметр("ссылка",					ссылка);
	Запрос.УстановитьПараметр("МоментВремени",			параметры.МоментВремени);
	Запрос.УстановитьПараметр("МоментВремениИсключая",	Новый Граница(параметры.МоментВремени,ВидГраницы.Исключая));
	Запрос.УстановитьПараметр("Организация",			ПараметрыЗапроса.Организация);
	Запрос.УстановитьПараметр("Район",					ПараметрыЗапроса.Район);
	Запрос.УстановитьПараметр("Договор",				ПараметрыЗапроса.Договор);
	Запрос.УстановитьПараметр("ОбъектРасчета",			ссылка.ОбъектРасчета);
	Запрос.УстановитьПараметр("МКД",					ссылка.ОбъектРасчета.МКД);
	Запрос.УстановитьПараметр("МассивТочек",			МассивТочек);
	Запрос.УстановитьПараметр("РасчитываемаяТочка",     ссылка.РассчитываемаяТочкаУчета);
	Запрос.МенеджерВременныхТаблиц 	= МВТ;     	
	
	ПараметрыЗапроса 	= Параметры.ПараметрыЗапроса;
	для каждого ПараметрЗапроса из ПараметрыЗапроса цикл
		Запрос.УстановитьПараметр(ПараметрЗапроса.ключ,ПараметрЗапроса.Значение);
	КонецЦикла;
	Запрос.УстановитьПараметр("ПериодНачисления",	Параметры.ПериодНачисления); 	
	Запрос.Текст 					="ВЫБРАТЬ
	             					 |	энргВладельцыТочекУчетаСрезПоследних.ТочкаУчета КАК ТочкаУчета
