	для Каждого стр из РесурсыРегистра цикл 		
		ЕстьРесурс 										= Ресурсы.найти(стр) <> Неопределено;
		Если ЕстьРесурс и НЕ СтрНайти(ПараметрыНорматива,(стр + ",")) = 0 Тогда
